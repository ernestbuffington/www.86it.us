# To prevent access to .env and other files
<Files .*>
# Apache 2.2
<IfModule !mod_authz_core.c>
Order deny,allow
Deny from all
</IfModule>
# Apache 2.4
<IfModule mod_authz_core.c>
Require all denied
</IfModule>
</Files>

<IfModule mod_alias.c>
RedirectMatch 301 ^/dev_includes/igniter/$ https://php-nuke-titanium.86it.us/dev_includes/igniter/public
</IfModule>